<Window x:Class="MLView.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MLView"
        xmlns:views="clr-namespace:MLView.Views"
        mc:Ignorable="d" WindowStartupLocation="CenterScreen"
        Title="MLView" Height="650" Width="900">
    <DockPanel>
        <TabControl>
            <TabItem Header="TestArea">
                <local:TestAreaView/>
            </TabItem>
            <TabItem Header="LinearRegression">
                <views:LinearRegressionView/>
            </TabItem>
            <TabItem Header="LogisticRegression">
                <views:LogisticRegressionView/>
            </TabItem>
            <TabItem Header="NeuralNetwork">

            </TabItem>
        </TabControl>
    </DockPanel>
</Window>
